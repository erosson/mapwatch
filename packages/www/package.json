{
  "name": "@mapwatch/www",
  "version": "1.0.0",
  "main": "build/elm.js",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "//prepare": "npm packages run `yarn build` before publishing; my github fork does not. manually build during install for now; remove this once @wolfadex/fluent-web below is no longer a fork",
    "prepare": "cd ../../node_modules/@wolfadex/fluent-web && yarn && yarn build",
    "_build:version": "bash scripts/_build/version",
    "build": "yarn _build:version && elm-app build",
    "start": "yarn _build:version && bash -c \"ELM_DEBUGGER=false elm-app start\"",
    "test": "elm-app test",
    "clean": "rm -rf build tmp"
  },
  "dependencies": {
    "@mapwatch/datamine": "1.0.0",
    "@mapwatch/rss": "1.0.0",
    "@wolfadex/fluent-web": "erosson/fluent-web#cc99ac678bf3578e2d83c36fbf55fad648354d23"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "create-elm-app": "^4.2.7",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.0",
    "sakura.css": "^1.0.0",
    "scripty": "^1.7.2"
  }
}
